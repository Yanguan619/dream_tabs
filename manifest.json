{
  "name": "梦想标签收纳盒",
  "description": "一款为节约浏览器内存而开发的标签收纳工具。",
  "version": "1.2",
  "manifest_version": 2,
  "icons": {
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "收纳全部标签"
  },
  "background": {
    "scripts": [
      "js/common.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "chrome://favicon/",
    "contextMenus",
    "storage",
    "tabs"
  ],
  "homepage_url": "https://github.com/ryanker/dream_tabs",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+D",
        "mac": "MacCtrl+Shift+D"
      },
      "description": "打开梦想标签收纳盒"
    }
  }
}


